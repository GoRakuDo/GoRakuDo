{
  "seoOptimization": {
    "titleTemplate": "{{articleTitle}} | GoRakuDo",
    "descriptionTemplate": "{{articleDescription}}",
    "baseKeywords": [
      "Pembelajaran Bahasa Jepang",
      "GoRakuDo",
      "Bahasa Jepang",
      "Panduan Pembelajaran"
    ]
  },
  "structuredData": {
    "breadcrumb": {
      "@context": "https://schema.org",
      "@type": "BreadcrumbList",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "Beranda",
          "item": "https://gorakudo.org"
        },
        {
          "@type": "ListItem",
          "position": 2,
          "name": "Dokumentasi",
          "item": "https://gorakudo.org/docs"
        },
        {
          "@type": "ListItem",
          "position": 3,
          "name": "{{articleTitle}}",
          "item": "{{articleUrl}}"
        }
      ]
    }
  }
}
